local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/48973854274252/Z/main/Orion')))() --This Will Load The Script Code
local Player = game.Players.LocalPlayer --This Will Reveal The Player Name
  local Window = OrionLib:MakeWindow({
		Name = "Fps unlocker fixed☄",
		HidePremium = false,
		SaveConfig = true,
		ConfigFolder = "OrionTest",
        IntroText = "- Loading -"       
}) --This Will Load The Script Hub

function MakeScriptHub()
         loadstring(game:HttpGet("https://raw.githubusercontent.com/48973854274252/-/main/FPS%20UP"))() --Put The Script That Will Load If The Key Is Correct Here
end

OrionLib:MakeNotification({
	Name = "Verify",
	Content = "Enter Verify Into the Box above Or Press Enter⏎ ‍ "..Player.Name..".",
	Image = "rbxassetid://4483345998",
	Time = 5
}) --Notification

getgenv().Key = "Verify" --Put The Correct Key Here
getgenv().KeyInput = "string" --Require For The Key To Work

local Tab = Window:MakeTab({
	Name = "ꤥ",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
}) --Making A Tab

Tab:AddTextbox({
	Name = "Verify here",
	Default = "Verify",
	TextDisappear = true,
	Callback = function(Value)
		getgenv().KeyInput = Value
	end	  
}) --You Will Enter The Key Here

Tab:AddButton({
    Name = "⏎ E N T E R",
    Callback = function()
        if getgenv().KeyInput == getgenv().Key then
            OrionLib:MakeNotification({
                Name = "Verifying..",
                Content = "Checking, please wait",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
            wait(2)
            OrionLib:MakeNotification({
                Name = "Loaded",
                Content = "Correct",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
            wait(1)
            OrionLib:Destroy()
            wait(.3)
            MakeScriptHub()
        else
           OrionLib:MakeNotification({
                Name = "Verifying..",
                Content = "Checking, please wait",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
            wait(2)
            OrionLib:MakeNotification({
                Name = "Incorrect",
                Content = "Retry ҉‍",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end   
}) 
    
OrionLib:Init() --Require If The Script Is Done
